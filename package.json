{
  "name": "@golface/pm2-stackdriver",
  "version": "0.0.6",
  "description": "PM2 stackdriver logging module",
  "main": "app.js",
  "dependencies": {
    "@google-cloud/logging": "^4.5.2",
    "pm2": "3.5.0",
    "pmx": "1.6.7"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.golface.com.tw:golface-family/developer/web-developer/pm2-stackdriver.git"
  },
  "config": {
    "pm2_stackdriver_on_google": false,
    "pm2_stackdriver_enabled": false,
    "pm2_stackdriver_log_name": "pm2-info",
    "pm2_stackdriver_error_log_name": "pm2-error"
  },
  "apps": [
    {
      "merge_logs": true,
      "max_memory_restart": "200M",
      "script": "app.js"
    }
  ],
  "author": "YMH",
  "license": "MIT"
}
